<Window x:Class="AUTO_EDUNET.NET.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AUTO_EDUNET.NET"
        mc:Ignorable="d"
        Title="MainWindow" Height="640" Width="360">
	<Window.Background>
		<LinearGradientBrush EndPoint="0.0,0.0" StartPoint="0.0,1">
			<GradientStop Color="#FF8888FF"/>
			<GradientStop Color="#FFFF8888" Offset="1"/>
		</LinearGradientBrush>
	</Window.Background>
</Window>
